# Environment variables for production
NEXT_PUBLIC_IGNORE_BUILD_ERROR=true
NEXT_PUBLIC_NETWORK=sepolia
NEXT_PUBLIC_DEFAULT_CHAIN_ID=11155111

# Disable telemetry in production
NEXT_TELEMETRY_DISABLED=1

# Disable Next.js analytics
NEXT_PUBLIC_ANALYTICS_ID=

# Disable React's development warnings in production
NODE_ENV=production

# Disable React's StrictMode in production
NEXT_PUBLIC_STRICT_MODE=false

# Disable source maps in production
GENERATE_SOURCEMAP=false

# Disable TypeScript type checking in production
TSC_COMPILE_ON_ERROR=false

# Disable ESLint in production
ESLINT_NO_DEV_ERRORS=false

# Disable linting during build
NEXT_PUBLIC_ESLINT_IGNORE_DURING_BUILDS=true

# Disable TypeScript type checking during build
NEXT_PUBLIC_TSC_COMPILE_ON_ERROR=false
